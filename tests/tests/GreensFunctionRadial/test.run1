#juDFT Testscript

jt::copyfile("files/inp.xml",$workdir);
jt::copyfile("files/JUDFT_WARN_ONLY",$workdir);


jt::testrun("$executable ",$workdir);

#now test output
$result=jt::test_fileexists("$workdir/out");
$result+=jt::test_grepexists("$workdir/out","it=  1  is completed");
$result+=jt::test_grepnumber("$workdir/out","l--> 2 Contour(default)    Spin-Up trace:",": *([^ ]*)",4.0540,0.0005);
$result+=jt::test_grepnumber("$workdir/out","l--> 2 Contour(default)    Spin-Down trace:",": *([^ ]*)",1.8645,0.0005);
$result+=jt::test_grepnumber("$workdir/out","l--> 1 Contour(default)    Spin-Up trace:",": *([^ ]*)",3.1967,0.0005);
$result+=jt::test_grepnumber("$workdir/out","l--> 1 Contour(default)    Spin-Down trace:",": *([^ ]*)",3.2171,0.0005);

jt::stageresult($workdir,$result,"1");
