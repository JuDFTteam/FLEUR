#juDFT Testscript

jt::copyfile("files/inp-1.xml","$workdir/inp.xml");
jt::copyfile("files/JUDFT_WARN_ONLY",$workdir);


jt::testrun("$executable ",$workdir);

#now test output
$result=jt::test_fileexists("$workdir/out");
$result+=jt::test_grepexists("$workdir/out","it=  1  is completed");
$result+=jt::test_grepnumber("$workdir/out","l--> 2 Contour(default)    Spin-Up trace:",": *([^ ]*)",4.0387,0.0005);
$result+=jt::test_grepnumber("$workdir/out","l--> 2 Contour(default)    Spin-Down trace:",": *([^ ]*)",1.8773,0.0005);
$result+=jt::test_grepnumber("$workdir/out","l--> 1 Contour(default)    Spin-Up trace:",": *([^ ]*)",3.1968,0.0005);
$result+=jt::test_grepnumber("$workdir/out","l--> 1 Contour(default)    Spin-Down trace:",": *([^ ]*)",3.2177,0.0005);

jt::stageresult($workdir,$result,"1");
